{
  "name": "electron-auto-update-testing",
  "version": "1.14.1",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "release": "electron-builder --publish=always"
  },
  "build": {
    "linux": {
      "publish": ["github"]
    },
    "mac": {
      "publish": ["github"]
    },
    "win": {
      "publish": ["github"]
    }
  },
  "publish": [
    {
      "provider": "github",
      "owner": "anneeb",
      "repo": "https://github.com/anneeb/electron-auto-update-testing"
    }
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/anneeb/electron-auto-update-testing.git"
  },
  "devDependencies": {
    "electron": "^7.1.1",
    "electron-builder": "^21.2.0"
  }
}
